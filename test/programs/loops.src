int factorialWhile(int max) {
    int result = 1;
    int n = max;
    while(n != 1) {
        result = result * n;
        n = n - 1;
    }
    return result;
}

int whileSum(int iterations, int add) {
    int result = 0;
    int it = iterations;
    while(it > 0) {
        result = result + add;
        it = it - 1;
    }
    return result;
}

